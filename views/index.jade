extends layout

block content
  h1= title
  p Welcome to #{title}
  div.hub-activator(data-hub-activator)
    div
     form(action='/parseData', method='post')
       div(data-role='fieldcontain')
         fieldset(data-role='controlgroup')
           label(for='data') Upload your JSON data
              textarea(id='data', placeholder='data',name='data')
       div(data-role='fieldcontain')
         input(type='submit',value='Submit Json Data', data-transition='fade', data-theme='c')
